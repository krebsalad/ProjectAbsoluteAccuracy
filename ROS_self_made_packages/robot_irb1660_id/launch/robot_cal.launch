<launch>

  <!-- Launch 3D sensors -->
    <!-- camera launch -->
  <include file="$(find realsense2_camera)/launch/rs_rgbd.launch"/>

  <!-- Launch robot. -->
  <include file="$(find robot_irb1660_id)/launch/planning_context.launch" >
    <arg name="load_robot_description" value="true" />
  </include>

  <include file="$(find robot_irb1660_id)/launch/robot_interface_download_irb1600_6_12.launch" >
      <arg name="robot_ip" value="10.128.187.52"/>
  </include>
  
  <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" />

  <include file="$(find robot_irb1660_id)/launch/move_group.launch">
    <arg name="publish_monitored_planning_scene" value="true" />
  </include>

  <include file="$(find robot_irb1660_id)/launch/moveit_rviz.launch">
    <arg name="config" value="true"/>
  </include>

</launch>

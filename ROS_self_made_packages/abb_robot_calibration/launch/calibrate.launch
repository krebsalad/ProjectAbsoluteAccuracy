<launch>
	<!-- launch calibrate phase-->
	<node pkg="robot_calibration" type="calibrate" name="calibrate"
		args="--from-bag /tmp/abb_robot_calibration/calibration_data_2018-11-23-13-33-13.bag"
		output="screen" required="true">
	<rosparam file="$(find abb_robot_calibration)/config/calibrate.yaml" command="load"/>
	</node>

	<!-- launch robot_cal.launch from robot package -->
  	<!-- include file="$(find robot_irb1660_id)/launch/robot_cal.launch"/ -->
</launch>
